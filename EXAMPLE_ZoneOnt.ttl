@prefix bldg: <https://example.org/building#> .
@prefix zoneont: <https://w3id.org/zoneont#> .

bldg:Library_MainHall a zoneont:ThermalZone ;
    zoneont:hasThermalInteraction zoneont:ConvectiveZone ;
    zoneont:hasOccupancyType zoneont:InstitutionalZone ;
    zoneont:floorArea 250.0 ;
    zoneont:ceilingHeight 3.5 ;
    zoneont:thermalMassLevel "High" ;
    zoneont:windowToWallRatio 0.35 ;
    zoneont:peakOccupancy 60 ;
    zoneont:avgOccupancyHours 10.5 ;
    zoneont:internalGainsDensity 5.8 ;
    zoneont:ventilationRate 1.2 ;
    zoneont:preferredHeatingRange "20–22°C" ;
    zoneont:preferredCoolingRange "24–26°C" .
